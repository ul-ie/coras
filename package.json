{
  "name": "coras",
  "version": "1.1.0",
  "description": "CÃ³ras is the design system for University of Limerick.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -r dist",
    "start": "npm run 11ty-serve & npm run tw-watch",
    "build": "npm run clean && npm run 11ty-build && npm run tw-build",
    "icons": "npm run icons-update && npm run icons-copy",
    "icons-copy": "cp -r ./node_modules/@awesome.me/kit-c3571caea7/icons/svgs/* ./inc/assets/icons",
    "icons-update": "npm update '@awesome.me/kit-c3571caea7'",
    "11ty-serve": "npx @11ty/eleventy --serve",
    "11ty-build": "npx @11ty/eleventy",
    "tw-watch": "npx @tailwindcss/cli -i ./inc/css/index.css -o ./dist/css/index.css --watch",
    "tw-build": "npx @tailwindcss/cli -i ./inc/css/index.css -o ./dist/css/index.css --minify"
  },
