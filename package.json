  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@tailwindcss/cli": "^4.1.17",
    "js-yaml": "^4.1.1",
    "tailwindcss": "^4.1.17",
    "twig": "^1.17.1"
  },
